@page "/wiki/add"
@model velociraptor.Pages.AddModel
@{
    ViewData["Title"] = "Add";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <br/>
    <div class="input-group flex-nowrap">
        <input type="text" class="form-control" placeholder="Article title" asp-for="ArticleTitle" aria-describedby="addon-wrapping">
    </div>
    <br/>
    <input type="submit" class="btn btn-primary"/>
</form>

